{
  "expo": {
    "name": "yahwe-eita",
    "slug": "yahwe-eita",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.berthengineering.yahweeita"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.berthengineering.yahweeita"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "imageWidth": 150
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/logo.png",
            "./assets/images/berth.png",
            "./assets/images/momo.png",
            "./assets/images/berth_white.png"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "57067454-590f-4aeb-9ea8-18672c33e4ae"
      }
    }
  }
}
